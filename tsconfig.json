{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": true,
    "plugins": [
      {"name": "next"}
    ]
  },
  "include": [
    "./dist/types/**/*.ts",
    "./next-env.d.ts"
  ],
  "exclude": [
    "./node_modules"
  ],
  "esModuleInterop": true,
  "jsx": "preserve",
  "allowJs": true,
  "forceConsistentCasingInFileNames": true,
  "incremental": true,
}
